generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model count {
  id    Int  @id @default(autoincrement())
  value Int?
}

model player {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  age   Int     @default(18)
}
